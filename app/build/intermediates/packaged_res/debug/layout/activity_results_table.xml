<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#616161"
    tools:context=".ResultsTable">

    <TextView
        android:id="@+id/love_calc"
        android:layout_width="372dp"
        android:layout_height="87dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/quintessential"
        android:gravity="center"
        android:text="Results"
        android:textColor="#D50000"
        android:textSize="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TableLayout
        android:layout_width="388dp"
        android:layout_height="645dp"
        android:background="#ffffff"
        android:shrinkColumns="*"
        android:stretchColumns="*"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.183"
        tools:ignore="MissingConstraints">


        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:text="Language"
                android:textColor="#000000"
                android:textSize="30dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="80dp"
                tools:layout_conversion_absoluteWidth="218dp"
                tools:layout_editor_absoluteX="2dp"
                tools:layout_editor_absoluteY="0dp" />

            <TextView
                android:id="@+id/result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:text="Result"
                android:textColor="#000000"
                android:textSize="30dp"
                android:textStyle="bold"
                tools:layout_conversion_absoluteHeight="80dp"
                tools:layout_conversion_absoluteWidth="171dp"
                tools:layout_editor_absoluteX="220dp"
                tools:layout_editor_absoluteY="0dp" />


        </TableRow>


        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_weight="1"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:text="Java"
                android:textColor="#000000"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/java_text"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:textColor="#000000"
                android:textSize="23dp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_weight="1"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:text="JavaScript"
                android:textColor="#000000"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/javascript_text"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:textColor="#000000"
                android:textSize="23dp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_weight="1"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:text="C"
                android:textColor="#000000"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/c_text"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:textColor="#000000"
                android:textSize="23dp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_weight="1"
                android:background="@drawable/border"
                android:gravity="center"
                android:padding="20dip"
                android:text="HTML5"
                android:textColor="#000000"
                android:textSize="23dp" />

            <TextView
                android:id="@+id/html_text"
                android:layout_weight="1"
